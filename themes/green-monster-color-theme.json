                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "variable.other.constant",
                "constant.other.symbol",
                "constant.other.key",
                "constant.other.color",
                "constant.other"
            ],
            "settings": {
                "foreground": "#5AE640"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder",
                "variable.other",
                "variable.parameter",
                "variable.other.object"
            ],
            "settings": {
                "foreground": "#e0e0e0"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#666666",
                "fontStyle": "italic"
